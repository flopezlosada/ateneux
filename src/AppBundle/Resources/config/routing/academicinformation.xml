<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="academicinformation_index" path="/" methods="GET">
        <default key="_controller">AppBundle:AcademicInformation:index</default>
    </route>

    <route id="academicinformation_show" path="/{id}/show" methods="GET">
        <default key="_controller">AppBundle:AcademicInformation:show</default>
    </route>

    <route id="academicinformation_new" path="/new/{meeting_id}" methods="GET|POST">
        <default key="_controller">AppBundle:AcademicInformation:new</default>
    </route>

    <route id="academicinformation_associate" path="/associate/{meeting_id}" methods="GET|POST">
        <default key="_controller">AppBundle:AcademicInformation:associate</default>
    </route>

    <route id="academicinformation_edit" path="/{id}/edit" methods="GET|POST">
        <default key="_controller">AppBundle:AcademicInformation:edit</default>
    </route>

    <route id="academicinformation_delete" path="/{id}/delete" methods="DELETE">
        <default key="_controller">AppBundle:AcademicInformation:delete</default>
    </route>

</routes>
