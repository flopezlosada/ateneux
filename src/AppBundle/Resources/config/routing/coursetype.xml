<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="coursetype_index" path="/" methods="GET">
        <default key="_controller">AppBundle:CourseType:index</default>
    </route>

    <route id="coursetype_show" path="/{id}/show" methods="GET">
        <default key="_controller">AppBundle:CourseType:show</default>
    </route>

    <route id="coursetype_new" path="/new" methods="GET|POST">
        <default key="_controller">AppBundle:CourseType:new</default>
    </route>

    <route id="coursetype_edit" path="/{id}/edit" methods="GET|POST">
        <default key="_controller">AppBundle:CourseType:edit</default>
    </route>

    <route id="coursetype_delete" path="/{id}/delete" methods="DELETE">
        <default key="_controller">AppBundle:CourseType:delete</default>
    </route>

</routes>
