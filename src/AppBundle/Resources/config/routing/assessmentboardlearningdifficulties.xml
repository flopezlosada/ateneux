<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="assessmentboardlearningdifficulties_index" path="/" methods="GET">
        <default key="_controller">AppBundle:AssessmentBoardLearningDifficulties:index</default>
    </route>

    <route id="assessmentboardlearningdifficulties_show" path="/{id}/show" methods="GET">
        <default key="_controller">AppBundle:AssessmentBoardLearningDifficulties:show</default>
    </route>

    <route id="assessmentboardlearningdifficulties_new" path="/new/{id}" methods="GET|POST">
        <default key="_controller">AppBundle:AssessmentBoardLearningDifficulties:new</default>
    </route>

    <route id="assessmentboardlearningdifficulties_new_group" path="/new_group/{assessment_board_id}/{difficulty_type_id}" methods="GET|POST">
        <default key="_controller">AppBundle:AssessmentBoardLearningDifficulties:new_group</default>
    </route>

    <route id="assessmentboardlearningdifficulties_create" path="/create/{assessment_board_id}/{difficulty_type_id}" methods="GET|POST">
        <default key="_controller">AppBundle:AssessmentBoardLearningDifficulties:create</default>
    </route>

    <route id="assessmentboardlearningdifficulties_edit" path="/{id}/edit" methods="GET|POST">
        <default key="_controller">AppBundle:AssessmentBoardLearningDifficulties:edit</default>
    </route>

    <route id="assessmentboardlearningdifficulties_delete" path="/{id}/delete" methods="DELETE">
        <default key="_controller">AppBundle:AssessmentBoardLearningDifficulties:delete</default>
    </route>

    <route id="assessmentboardlearningdifficulties_fast_delete" path="/{id}/fast_delete" methods="GET">
        <default key="_controller">AppBundle:AssessmentBoardLearningDifficulties:fast_delete</default>
    </route>

</routes>
