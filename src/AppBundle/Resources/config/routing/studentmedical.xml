<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="studentmedical_index" path="/" methods="GET">
        <default key="_controller">AppBundle:StudentMedical:index</default>
    </route>

    <route id="studentmedical_show" path="/{id}/show" methods="GET">
        <default key="_controller">AppBundle:StudentMedical:show</default>
    </route>

    <route id="studentmedical_new" path="/new/{student_id}" methods="GET|POST">
        <default key="_controller">AppBundle:StudentMedical:new</default>
    </route>

    <route id="studentmedical_edit" path="/{id}/edit" methods="GET|POST">
        <default key="_controller">AppBundle:StudentMedical:edit</default>
    </route>

    <route id="studentmedical_delete" path="/{id}/delete" methods="DELETE">
        <default key="_controller">AppBundle:StudentMedical:delete</default>
    </route>

</routes>
